    "@supabase/supabase-js": "^2.49.1",
    "@vercel/analytics": "^1.5.0",
    "glob": "^10.3.10",
    "gray-matter": "^4.0.3",
    "next": "15.2.1",
    "next-intl": "^3.26.5",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/glob": "^8.1.0",
    "@types/node": "^22",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "eslint": "^9",
    "eslint-config-next": "15.2.1",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "typescript": "^5"
